<head>
    <title>story lines | {{title}}</title>
    {{> main/head-block}}
    <!-- dashboard page js script -->
    <script type="text/javascript" src="/assets/js/dash.js"></script>
</head>
<body class="max mx-auto">
  <header>
    {{> main/nav-menu-block}}
  </header>
  <main>
    <section class="title-container mx-auto">
      <h1 class="title mx-auto">Welcome, Jane! </h1>

    </section>
    <section class="section-container mx-auto text-center">
      <p class="section-title">Active Projects</p>
      {{#each project }}
        {{#each this }}
            <div>{{ title }} </div>
        {{/each}}
      {{/each}}
      <div id="dynamic-project"></div>
    </section>
    <section class="section-container mx-auto text-center">
      <p class="section-title">Active Stories</p>
      
      {{#each project }}
        {{#each this.Stories }}
            <div>{{ this.dataValues.title }} <br/>
            {{ this.dataValues.description }} </div>
        {{/each}}
      {{/each}}
    
      <div id="dynamic-story"></div>
    </section>


    
  </main>
</body>