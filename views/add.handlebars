<head>
    <title>story lines | {{#each array}}{{title}}{{/each}}</title>
    {{> main/head-block}}
    <!-- story page js script -->
    <script type="text/javascript" src="/assets/js/add.js"></script>
</head>
<body class="max mx-auto">
  <header>
    {{> main/nav-menu-block}}
  </header>
  <main>
    <section class="title-container mx-auto">
      <h1 id="project-title" class="title mx-auto">{{#each array}}{{title}}{{/each}}</h1>
    </section>
    <section class="section-container mx-auto">
      <p class="section-title">Title</p>
      <input id="title" class="full" type="text" />
      <p class="section-title">Description</p>
      <textarea id="project-description" class="full" rows="5"></textarea>
      <p class="project-owner">Owner</p>
      <select id="project-owner" class="full">
        {{#each array}}
          {{#each user}}
            {{> add/option}}
          {{/each}}
        {{/each}}
      </select>
      <div class="row-container mx-auto text-right">
        <button type="submit" id="submit-btn" class="button mr-0">Add Project</button>
      </div>
  </section>
  </main>
</body>